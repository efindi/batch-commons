<?xml version="1.0"?>
<!DOCTYPE import-control PUBLIC
        "-//Checkstyle//DTD ImportControl Configuration 1.4//EN"
        "https://checkstyle.org/dtds/import_control_1_4.dtd">

<import-control pkg="com.efindi" strategyOnMismatch="allowed">

    <!--Use Vavr, if you need something for interop with other libs use fully qualified names like java.util.Set-->
    <disallow pkg="java.util.stream"/>
    <disallow class="java.util.List"/>
    <disallow class="java.util.ArrayList"/>
    <disallow class="java.util.LinkedList"/>
    <disallow class="java.util.Set"/>
    <disallow class="java.util.HashSet"/>
    <disallow class="java.util.Map"/>
    <disallow class="java.util.HashMap"/>

</import-control>
